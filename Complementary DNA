#include <string>
#include <iostream>
#include <vector>
#include <bits/stdc++.h>

std::string DNAStrand(const std::string& dna)
{
  
  
    int size = dna.size();
  std::cout<<size<<"\n";
    //std::cout << dna.substr(0, size);
  
  //turning strings into char so i can compare
  
char a = 'A';
char t = 'T';
char g = 'G';
char c = 'C';
  
  //creating array to store number in
   std::vector<char> x;
  
  
  //while the 
for (int i = 0; i<=size; i++){
  char digit = dna.at(i);
 
  
  //if a, become t
  if (a == digit) {
  x.push_back(t);
    dna.pop_back();
    }
  
  //if t, become a
  if (t == digit) {
  x.push_back(a);
    }
  
  //if g, become c
  if (g == digit) {
  x.push_back(c);
  }
  
  //if c, become g
  if (c == digit) {
  x.push_back(g);
    
}
  
  
    
    
  //return array for loop (i = number in array)
  std::string y(x.begin(), x.end()); 
  return y;
  
}
  
std::cout<<size<<"\n";
}

