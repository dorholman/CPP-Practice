#include <string>
#include <iostream>
#include <vector>
#include <bits/stdc++.h>

std::string DNAStrand(const std::string& DNA)
{
  
  //create new var
  std::string dna = DNA;
  
  //create size
    int size = dna.size();
  
  
  std::cout<<dna<<"\n"; //print dna
  
  //turning strings into char so i can compare
  
char a = 'A';
char t = 'T';
char g = 'G';
char c = 'C';
  
  //creating array to store number in
   std::vector<char> x;
  
  
 
for (int i = size - 1; i>=0; i--){
  
 std::cout<<i<<"\n";

  
  //if a, become t
  if (a == dna.at(i)) {
  x.push_back(t);
    std::cout<<dna.at(i)<<"\n";
    
   
    
    }
  
  
  
  //if t, become a
  else if (t == dna.at(i)) {
  x.push_back(a);
    std::cout<<dna.at(i)<<"\n";
 
    }
  
  //if g, become c
  else if (g == dna.at(i)) {
  x.push_back(c);
    std::cout<<dna.at(i)<<"\n";

    
  }
  
  //if c, become g
  else if (c == dna.at(i)) {
  x.push_back(g);
    std::cout<<dna.at(i)<<"\n";
 
    
}
 
std::cout<<"\n";
  
    
    
  //return array for loop (i = number in array)
  std::string y(x.begin(), x.end()); 
  

  
  
}
  

return "a";
}
}
  
std::cout<<size<<"\n";
}

